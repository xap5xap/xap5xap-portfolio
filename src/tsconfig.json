    "app/contact/contact.component.ts",
    "app/contact/index.ts",
    "app/contact/shared/index.ts",
    "app/customers/customers.component.spec.ts",
    "app/customers/customers.component.ts",
    "app/customers/index.ts",
    "app/customers/shared/index.ts",
    "app/environment.ts",
    "app/footer/footer.component.spec.ts",
    "app/footer/footer.component.ts",
    "app/footer/index.ts",
    "app/footer/shared/index.ts",
    "app/index.ts",
    "app/intro/index.ts",
    "app/intro/intro.component.spec.ts",
