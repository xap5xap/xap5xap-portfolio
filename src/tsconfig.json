    "app/map/shared/index.ts",
    "app/navbar/index.ts",
    "app/navbar/navbar.component.spec.ts",
    "app/navbar/navbar.component.ts",
    "app/navbar/shared/index.ts",
    "app/references/index.ts",
    "app/references/references.component.spec.ts",
    "app/references/references.component.ts",
    "app/references/shared/index.ts",
    "app/services/index.ts",
    "app/services/services.component.spec.ts",
    "app/services/services.component.ts",
    "app/services/shared/index.ts",
    "app/shared/index.ts",
    "app/showcase/index.ts",
    "app/showcase/shared/index.ts",
    "app/showcase/showcase.component.spec.ts",
    "app/showcase/showcase.component.ts",
